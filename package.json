    "mongoose": "^5.9.7",
    "puppeteer": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.10",
    "@types/node": "^13.11.1",
    "@types/nodemon": "^1.19.0",
    "@types/puppeteer": "^2.0.1",
    "nodemon": "^2.0.3",
    "puppeteer-tsd": "0.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
